@model List<OutreachModule.Models.Examination>
@{
    ViewBag.Title = "Completed";
}
<h2>Completed Examinations</h2>
<ul>
    @if (Model.Count > 0)
    {
        foreach (var e in Model)
        {
            <li>@Html.ActionLink(e.Patient.mrn, "GoToExamination", new { examId = e.Id })</li>
        }
    }
    else
    {
        @Html.Raw("<p>No completed</p>")
    }
    <p>@Html.ActionLink("Back", "Index", "Home")</p>
</ul>