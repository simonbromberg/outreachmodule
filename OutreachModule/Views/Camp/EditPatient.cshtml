@model OutreachModule.Models.Patient

@{
    ViewBag.Title = "Edit Patient";
}

<h2>Edit Patient</h2>

@Html.Partial("~/Views/Shared/_Patient.cshtml")
<div>
    @Html.ActionLink("Back", "Patient", new { id = Model.Id })
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script>
        var mes = "@ViewBag.Message";
        if (mes != '') {
            window.alert(mes);
        }
    </script>
    @Scripts.Render("~/Scripts/birthdatepickersetup.js")
    @Scripts.Render("~/Scripts/blockenterkey.js")
}
