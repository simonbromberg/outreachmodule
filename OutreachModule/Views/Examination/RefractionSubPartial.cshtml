@model OutreachModule.Models.RefractionSubModel
<div class="form-group form-inline form-horizontal">
    <label class="control-label col-md-1">
       @Model.eye
    </label>
    <div class="form-group col-md-11">
        <div class="input-group" style="padding-right:10px;">
            <div class="input-group-addon">&nbsp;sph:</div>
            @Html.EditorFor(model => model.sph, new { htmlAttributes = new { @class = "form-control" } })
        </div>

        <div class="input-group" style="padding-right:10px;">
            <div class="input-group-addon">&nbsp;cyl :</div>
            @Html.EditorFor(model => model.cyl, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        <div class="input-group">
            <div class="input-group-addon">axis:</div>
            @Html.EditorFor(model => model.axis, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>

</div>
<br /><br />